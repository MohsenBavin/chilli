<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/beige_color_background"
    tools:context=".starterActivities.SignUpFieldStudy">

    <android.support.constraint.ConstraintLayout
        android:id="@+id/constraintLayout18"
        android:layout_width="match_parent"
        android:layout_height="351dp"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="20dp"
        android:background="@drawable/sign_up_background_shape"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.39" />

    <Spinner
        android:id="@+id/spinner_EducationalLevel"
        style="@style/MyEditText"
        android:background="@drawable/item_friend_data_shape"
        android:popupBackground="@color/colorBlueDarkOpaque"
        app:layout_constraintBottom_toBottomOf="@+id/constraintLayout18"
        app:layout_constraintEnd_toEndOf="@+id/constraintLayout18"
        app:layout_constraintStart_toStartOf="@+id/constraintLayout18"
        app:layout_constraintTop_toTopOf="@+id/constraintLayout18"
        app:layout_constraintVertical_bias="0.120000005">

    </Spinner>

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="35dp"
        android:layout_height="37dp"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        android:scaleType="fitXY"
        android:src="@drawable/drag"
        app:layout_constraintBottom_toTopOf="@+id/spinner_EducationalLevel"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.05"
        app:layout_constraintStart_toStartOf="@+id/spinner_EducationalLevel"
        app:layout_constraintTop_toBottomOf="@+id/spinner_EducationalLevel" />

    <Spinner
        android:id="@+id/spinner_FieldStudy"
        style="@style/MyEditText"
        android:layout_marginBottom="8dp"
        android:background="@drawable/item_friend_data_shape"
        android:popupBackground="@color/colorBlueDarkOpaque"
        app:layout_constraintBottom_toBottomOf="@+id/constraintLayout18"
        app:layout_constraintEnd_toEndOf="@+id/constraintLayout18"
        app:layout_constraintStart_toStartOf="@+id/constraintLayout18"
        app:layout_constraintTop_toBottomOf="@+id/spinner_EducationalLevel"
        app:layout_constraintVertical_bias="0.17000002">

    </Spinner>


    <ImageView
        android:id="@+id/imageView"
        android:layout_width="35dp"
        android:layout_height="37dp"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        android:scaleType="fitXY"
        android:src="@drawable/drag"
        app:layout_constraintBottom_toTopOf="@+id/spinner_FieldStudy"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.05"
        app:layout_constraintStart_toStartOf="@+id/spinner_FieldStudy"
        app:layout_constraintTop_toBottomOf="@+id/spinner_FieldStudy" />


    <Spinner
        android:id="@+id/spinner_zone"
        style="@style/MyEditText"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="8dp"
        android:background="@drawable/item_friend_data_shape"
        android:popupBackground="@color/colorBlueDarkOpaque"
        app:layout_constraintBottom_toBottomOf="@+id/constraintLayout18"
        app:layout_constraintEnd_toEndOf="@+id/constraintLayout18"
        app:layout_constraintStart_toStartOf="@+id/constraintLayout18"
        app:layout_constraintTop_toBottomOf="@+id/spinner_FieldStudy">

    </Spinner>


    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="35dp"
        android:layout_height="37dp"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        android:scaleType="fitXY"
        android:src="@drawable/drag"
        app:layout_constraintBottom_toBottomOf="@+id/spinner_zone"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.04"
        app:layout_constraintStart_toStartOf="@+id/spinner_zone"
        app:layout_constraintTop_toTopOf="@+id/spinner_zone" />


    <Button
        android:id="@+id/autocomplete_state"
        style="@style/MyEditText"
        android:layout_width="339dp"
        android:layout_height="54dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="8dp"
        android:maxLength="20"
        android:singleLine="true"
        android:text="استان"
        android:textColor="@color/colorDarkText"
        android:textSize="30sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/constraintLayout18"
        app:layout_constraintTop_toBottomOf="@+id/spinner_zone"
        app:layout_constraintVertical_bias="0.0">

    </Button>

    <Button
        android:id="@+id/buttonNextforStudy"
        style="@style/MyButtonInactive"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="8dp"
        android:gravity="center"
        android:text="بعدی"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout18" />

</android.support.constraint.ConstraintLayout>